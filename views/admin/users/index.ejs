<%- include('../../partials/header.ejs')%>
<%- include('../../partials/navbar.ejs')%>
<div class="container">
    <hr>
    <h2>Usuários</h2>
    <a class="btn btn-primary" href="/admin/users/create">Criar novo usuário</a>
    <hr>
    <table class="table table-bordered">
        <thead>
            <tr>
                <th>Id</th>
                <th>email</th>
            </tr>
        </thead>
        <tbody>
            <% users.forEach(users => { %>
                <tr>
                    <td><%= users.id  %></td>
                    <td><%= users.email %></td>
                </tr>
            <% }) %>
        </tbody>
    </table>
</div>

<%- include('../../partials/footer.ejs')%>