<body>
    <div class="container">
        <hr>
        <% result.articles.rows.forEach(article => { %>
            <div class="card">
                <div class="card-header">
                    <h2><%= article.title  %></h2>
                </div>
                <div class="card-body">
                    <a  href="/<%= article.slug %>" class="btn btn-primary" >Ler artigo</a>
                </div>
            </div>
            <br>
        <% }) %>
    </div>
</body>

